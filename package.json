    "koa-router": "^10.1.1",
    "koa-static": "^5.0.0",
    "koa2-history-api-fallback": "^0.1.0",
    "node-pre-gyp": "^0.12.0",
    "reflect-metadata": "^0.1.13",
    "sequelize": "^6.7.0",
    "sequelize-typescript": "^2.1.1",
    "sqlite3": "^5.0.2",
    "uuid": "^8.3.2",
    "vue": "^3.0.0",
    "vue-class-component": "^8.0.0-0",
    "vue-logger-plugin": "^2.1.3",
    "vue-router": "^4.0.0-0"
  },
  "devDependencies": {
