    "eslint-plugin-vue": "^7.0.0",
    "nodemon": "^2.0.13",
    "stylus": "^0.54.7",
    "stylus-loader": "^3.0.2",
    "ts-node": "^10.3.0",
    "typescript": "~4.1.5"
  },
  "eslintConfig": {
    "root": true,
    "env": {
      "node": true
    },
    "extends": [
      "plugin:vue/vue3-essential",
      "eslint:recommended",
      "@vue/typescript/recommended"
    ],
    "parserOptions": {
      "ecmaVersion": 2020
    },
    "rules": {}
  },
  "browserslist": [
    "> 1%",
    "last 2 versions",
    "not dead"
