{
  "name": "vue-koa-guide",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "serve-vue": "vue-cli-service serve",
    "serve-koa": "nodemon --watch server -e ts,tsx --exec ts-node ./server/app.ts",
    "build": "vue-cli-service build",
    "compile": "tsc",
    "buildAll": "npm run compile && npm run build",
    "start": "cd dist && node app.js"
  },
  "dependencies": {
